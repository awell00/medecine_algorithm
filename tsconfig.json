{
    "compilerOptions": {
        "skipLibCheck": true,
        "target": "esNext",
        "module": "commonjs",
        "moduleResolution": "node",
        "allowJs": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "outDir": "build",
        "rootDir": "src",
        "types": ["@prisma/client"],

        "noImplicitAny": true
    },
    "include": ["src",
        "src/model"
    ],
    "exclude": ["node_modules"]
}